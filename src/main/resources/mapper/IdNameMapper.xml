<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springbootdemo.mapper.IdNameMapper">
    <select id="queryByName" resultType="com.example.springbootdemo.pojo.IdName">
        select * from id_name where name=#{name}
    </select>
</mapper>
